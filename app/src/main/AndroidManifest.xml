<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhuoxin.simpletext">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HttpActivity" />
        <activity android:name=".webView.WebViewActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Intent.IntentActivity" />
        <activity android:name=".Okhttp.OkHttpActivity" />
        <activity android:name=".Notification.NotificationActivity" />
        <activity android:name=".Banner.BannerActivity"></activity>
        <activity android:name=".SQLite.SqliteActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Shard.SharedActivity"></activity>
    </application>

</manifest>